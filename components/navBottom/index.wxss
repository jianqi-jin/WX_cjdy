/* components/navBottom/index.wxss */
.navBottom{
  width: 100%;
  position: fixed;
  bottom: 0rpx;
  z-index: 10;
}
.navBox{
  width:702rpx;
  height:90rpx;
  background:rgba(255,255,255,1);
  box-shadow:5rpx 9rpx 20rpx 0rpx rgba(151,79,10,0.2);
  border-radius:45rpx;
  margin: 52rpx 24rpx 25rpx 24rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
}
.navBtn{
  /*
  float: left;
  height: 100%;
  border: 1px solid #ccc;
  padding: 5rpx 80rpx 0 80rpx;
  */
  flex-grow: 0;
  width: 24%;

}
.navBtnBox{
  height: 100%;
  text-align: center;
  margin-top: 8rpx;
}

.navImg{
  width: 50rpx;
  height: 50rpx;
} 
.navImgNoTitle{
  width: 90rpx;
  height: 90rpx;
  margin-top:-8rpx;
}
.navTitle{
  height:19rpx;
  font-size:20rpx;
  font-family:PingFang-SC-Medium;
  font-weight:500;
  color:rgba(102,102,102,1);
  margin:-6rpx auto;
}

.backshadow{
  border-radius: 50rpx;
  overflow: hidden;
  box-shadow:0rpx 0rpx 30rpx 2rpx #FAB742;
}